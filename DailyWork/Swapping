#include<iostream>
using namespace std;
void swap(int a,int b)
{
  a = a^b;
  b = a^b;
  a = a^b;
  cout<<"After Swapping"<<endl;
  cout<<"a = "<<a<<"; b = "<<b<<endl;
}
int main()
{ 
  int x,y;
  cout<<"Enter the value of a and b"<<endl;
  cin>>x>>y;
  cout<<"Before Swapping"<<endl;
  cout<<"a = "<<x<< "; b = "<<y<<endl;
  swap(x,y);
  return 0;
  
}
